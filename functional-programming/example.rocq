Inductive day: type := 
  | Monday
  | Tuesday
  | Wednesday 
  | Thursday 
  | Friday 
  | Saturday 
  | Sunday. 


Definition next_day (d: day) : day :=
  match d with
    | Monday => Tuesday
    | Tuesday => Wednesday
    | Wednesday => Thursday
    | Thursday => Friday
    | Friday => Monday 
    | Saturday => Monday 
    | Sunday => Monday 
  end.

